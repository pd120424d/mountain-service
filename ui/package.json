{
  "name": "ui",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false --code-coverage --browsers=ChromeHeadlessNoSandbox",
    "generate-models": "node scripts/generate-models.js",
    "generate-employee-models": "swagger-typescript-api generate -p http://localhost:8082/swagger.json -o src/app/shared/models/generated/employee --modular",
    "generate-employee-models-local": "swagger-typescript-api generate -p employee-swagger.json -o src/app/shared/models/generated/employee --modular",
    "generate-urgency-models": "swagger-typescript-api generate -p http://localhost:8083/swagger.json -o src/app/shared/models/generated/urgency --modular",
    "generate-urgency-models-local": "swagger-typescript-api generate -p urgency-swagger.json -o src/app/shared/models/generated/urgency --modular",
    "generate-activity-models": "swagger-typescript-api generate -p http://localhost:8084/swagger.json -o src/app/shared/models/generated/activity --modular",
    "generate-activity-models-local": "swagger-typescript-api generate -p activity-swagger.json -o src/app/shared/models/generated/activity --modular",
    "models:generate": "npm run generate-models"
  },
  "private": true,
  "dependencies": {
    "-": "^0.0.1",
    "@angular/animations": "^19.0.5",
    "@angular/cdk": "^18.2.10",
    "@angular/common": "^19.0.5",
    "@angular/compiler": "^19.0.5",
    "@angular/core": "^19.0.5",
    "@angular/forms": "^19.0.5",
    "@angular/material": "^18.2.10",
    "@angular/platform-browser": "^19.0.5",
    "@angular/platform-browser-dynamic": "^19.0.5",
    "@angular/router": "^19.0.5",
    "@auth0/angular-jwt": "^5.2.0",
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "bootstrap": "^5.3.3",
    "date-fns": "^4.1.0",
    "ngx-spinner": "^19.0.0",
    "ngx-toastr": "^19.0.0",
    "npm": "^11.4.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.0.5",
    "@openapitools/openapi-generator-cli": "^2.21.4",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.2.0",
    "json-server": "^1.0.0-beta.3",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "swagger-typescript-api": "13.0.28",
    "typescript": "~5.5.2"
  }
}
